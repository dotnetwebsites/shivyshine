@model Brand

@{   
    ViewData["Title"] = "Details";
    ViewData["PageTitle"] = "Details";
}


<div class="row">
    <div class="col-sm-12 col-md-12">
        <div class="panel panel-bd ">
            <div class="panel-body">
                <div class="form-group col-md-4">
                    @Html.LabelFor(model => model.BrandName)
                    @Html.EditorFor(model => model.BrandName, new { htmlAttributes = new { @class = "form-control", @disabled="disabled"} })
                </div>

                <div class="form-group col-md-12">
                    <a asp-area="Admin" asp-action="Index" class="btn btn-info">Back</a>
                    <a asp-area="Admin" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-info">Edit</a>                    
                </div>
                
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $(".Master").addClass("active");
        });
    </script>
}